<div role="navigation" class="navbar navbar-inverse container-header">
    <div class="container">
        <div class="navbar-header">
            <button data-target=".navbar-collapse" data-toggle="collapse" class="navbar-toggle collapsed" type="button">
                <span class="sr-only">切换导航</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a href="{{url "/"}}" class="navbar-brand"></a>
            <a href="{{url "/"}}" class="back-office-label"></a>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right">
                {{#each typeList}}
                    {{#compare this "webapp" operator="=="}}
                        <li>
                            <a href="{{url "/assets/webapp/"}}"><i class="icon-globe icon"></i> Web Applications</a>
                        </li>
                    {{else}}
                    {{/compare}}
                    {{#compare this "mobileapp" operator="=="}}
                        <li class="active">
                            <a href="{{url "/assets/mobileapp/"}}"><i class="icon-mobile-phone"></i> 移动应用</a>
                        </li>
                    {{else}}
                    {{/compare}}
                {{/each}}
                <li>
                    <span style="color:#a3abbe; padding:13px 0px 13px 20px; line-height:60px;"><i class="icon-user"></i> 欢迎您，
                        {{#ifCond cuser.tenantDomain "==" "carbon.super"}}
                            {{cuser.username}}
                        {{else}}
                            {{cuser.username}}@{{cuser.tenantDomain}}
                        {{/ifCond}}！
                     </span>
                </li>
                <li>
                                <a onclick="window.location.href='{{url "/logout"}}'" href="#"><img src="/publisher/themes/mobileapp/img/icon_logout.png" /></a>
                            </li>
            </ul>
        </div>
    </div>
</div>

